<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode"/>
    </handlers>
    <iisnode loggingEnabled="true" logDirectory="iisnode" />
    <rewrite>
      <rules>
        <rule name="Nextjs" stopProcessing="true">
          <match url=".*" />
          <action type="Rewrite" url="server.js"/>
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>